{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/js/jqueryui/",
      "files": [
        "jquery-ui.min.js",
        "jquery-ui.js",
        "themes/base/jquery-ui.css",
        "themes/base/theme.css",
        "themes/base/images/ui-icons_444444_256x240.png",
        "themes/base/images/ui-icons_555555_256x240.png",
        "themes/base/images/ui-icons_777620_256x240.png",
        "themes/base/images/ui-icons_777777_256x240.png",
        "themes/base/images/ui-icons_cc0000_256x240.png",
        "themes/base/images/ui-icons_ffffff_256x240.png"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/js/jquery/"
    }
  ,
{
  "provider": "cdnjs",
  "library": "Sortable@1.10.1",
  "destination": "wwwroot/js/Sortable/",
  "files": [
    "Sortable.min.js"
  ]
}
]
}